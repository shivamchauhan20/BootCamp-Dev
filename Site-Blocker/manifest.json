{
    "name": "Site-Blocker",
    "version": "1.0",
    "description": "Blocks the site after 10sec",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}